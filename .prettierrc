{
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"printWidth": 80,
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 2,
	"useTabs": false,
  	"arrowParens": "always",
    "htmlWhitespaceSensitivity": "css",
  	"insertPragma": false,
  	"jsxSingleQuote": true,
